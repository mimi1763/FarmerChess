<?xml version="1.0" encoding="utf-8"?>
<configuration>
    <startup> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.6.1"/>
    </startup>
    <appSettings>
      <add key="BlockCountX" value="50"/>
      <add key="BlockCountY" value="50"/>
      <add key="BlockSize" value="4"/>
      <add key="LineThickness" value="0"/>
      <!-- Colours -->
      <add key="BgColour" value="lightgreen"/>
      <add key="GridColour" value="green"/>
      <add key="OColour" value="blue"/>
      <add key="XColour" value="green"/>
    </appSettings>
</configuration>
